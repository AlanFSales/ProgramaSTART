algoritmo "Escolha"

var
 voto: literal
inicio
      Escreval("Digite seu voto")
      leia(voto)
      
      escolha voto
      caso "20"
           escreval("Voce votou no Alan")
      caso "30"
           escreval("Voce votou no Pernalongo")
      caso "40"
           escreval("Voce votou no Patolino")
      outrocaso
           escreval("Voto Nulo")
      fimescolha
fimalgoritmo
