algoritmo "Trabahando com Tipos"

var
   numero1, numero2, soma, subtracao, multiplicacao: inteiro
   divisao: real
   sucesso: logico
inicio

Escreva("N1 ")
leia(numero1)
Escreva("N2 ")
leia(numero2)

sucesso <- numero1 > 10
escreval(sucesso)

soma <- numero1 + numero2
subtracao <- numero1 - numero2
multiplicacao <- numero1 * numero2
divisao <- numero1 / numero2
fimalgoritmo
